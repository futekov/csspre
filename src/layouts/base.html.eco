    <header>
      <% if @document.url is '/': %> 
        <h1 class="site-title"><%= @site.title %></h1>
      <% else: %>
        <h2 class="site-title">
          <a href="/" class="logo__text big-link"><%= @site.title %></a>
        </h2>
      <% end %>
      <nav class="site-navigation">
        <% for page in @getCollection("pages").toJSON(): %>
          <a href="<%= page.url %>" class="<%= if page.id is @document.id then 'active' else 'inactive' %>">
            <%= page.title %>
          </a>
        <% end %>
      </nav>
    </header>
    <% if @document.url is '/': %> 
    <section class="site">
      <h2 class="page-title"><%= @document.title %></h2>
    <% else: %>
    <section class="site" itemscope itemtype="http://schema.org/Article">
      <h1 class="page-title" itemprop="name"><%= @document.title %></h1>
    <% end %>
      <%- @content %>
    </section>
